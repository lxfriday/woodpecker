{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "import/prefer-default-export": "off",
    "no-param-reassign": "off",
    "consistent-return": "off"
  }
}
